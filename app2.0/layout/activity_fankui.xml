<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:padding="30dp"
    tools:context=".fankui">

    <TextView
        android:id="@+id/tv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="20dp"
        android:textSize="25sp"
        android:textColor="@color/zi"
        android:gravity="center"
        android:text="结果反馈"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="94dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="28dp"
        android:textSize="18sp"
        android:text="·请勾选出本次歌单中你喜欢的歌"
        android:layout_below="@+id/tv1"
        android:textColor="@color/zi"
        android:layout_marginLeft="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="18dp"
        android:textSize="19sp"
        android:text="歌曲1"
        android:layout_below="@+id/tv2"
        android:textColor="@color/zi"
        android:layout_marginLeft="20dp" />

    <TextView
        android:id="@+id/tv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="18dp"
        android:textSize="19sp"
        android:text="歌曲2"
        android:layout_below="@+id/tv3"
        android:textColor="@color/zi"
        android:layout_marginLeft="20dp" />

    <TextView
        android:id="@+id/tv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="18dp"
        android:textSize="19sp"
        android:text="歌曲3"
        android:layout_below="@+id/tv4"
        android:textColor="@color/zi"
        android:layout_marginLeft="20dp" />

    <TextView
        android:id="@+id/tv6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="18dp"
        android:textSize="19sp"
        android:text="歌曲4"
        android:textColor="@color/zi"
        android:layout_below="@+id/tv5"
        android:layout_marginLeft="20dp" />

    <TextView
        android:id="@+id/tv7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="18dp"
        android:textSize="19sp"
        android:text="歌曲5"
        android:textColor="@color/zi"
        android:layout_below="@+id/tv6"
        android:layout_marginLeft="20dp" />


    <TextView
        android:gravity="center"
        android:id="@+id/heart_1"
        android:layout_width="35dp"
        android:layout_height="35.0dip"
        android:layout_toRightOf="@+id/tv3"
        android:layout_below="@+id/tv2"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="12dp"
        android:paddingLeft="15.0dip"
        android:paddingRight="15.0dip"
        android:background="@drawable/heart_1"/>

    <TextView
        android:id="@+id/face_1"
        android:layout_width="35dp"
        android:layout_height="35.0dp"
        android:layout_toRightOf="@+id/heart_1"
        android:layout_below="@+id/tv2"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="12dp"
        android:paddingLeft="15.0dip"
        android:paddingRight="15.0dip"
        android:background="@drawable/face_1" />

    <TextView
        android:gravity="center"
        android:id="@+id/heart_2"
        android:layout_width="35dp"
        android:layout_height="35.0dip"
        android:background="@drawable/heart_2"
        android:layout_toRightOf="@+id/tv4"
        android:layout_below="@+id/tv3"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="12dp"
        android:paddingLeft="15.0dip"
        android:paddingRight="15.0dip"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/face_2"
        android:layout_width="35dp"
        android:layout_height="35.0dp"
        android:layout_below="@+id/face_1"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:layout_toRightOf="@+id/heart_2"
        android:background="@drawable/face_2"
        android:paddingLeft="15.0dip"
        android:paddingRight="15.0dip" />

    <TextView
        android:gravity="center"
        android:id="@+id/heart_3"
        android:layout_width="35dp"
        android:layout_height="35.0dip"
        android:background="@drawable/heart_3"
        android:layout_toRightOf="@+id/tv5"
        android:layout_below="@+id/tv4"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="12dp"
        android:paddingLeft="15.0dip"
        android:paddingRight="15.0dip"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/face_3"
        android:layout_width="35dp"
        android:layout_height="35.0dp"
        android:layout_toRightOf="@+id/heart_2"
        android:layout_below="@+id/face_2"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:paddingLeft="15.0dip"
        android:paddingRight="15.0dip"
        android:background="@drawable/face_3" />

    <TextView
        android:gravity="center"
        android:id="@+id/heart_4"
        android:layout_width="35dp"
        android:layout_height="35.0dip"
        android:background="@drawable/heart_4"
        android:layout_toRightOf="@+id/tv6"
        android:layout_below="@+id/tv5"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="12dp"
        android:paddingLeft="15.0dip"
        android:paddingRight="15.0dip"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/face_4"
        android:layout_width="35dp"
        android:layout_height="35.0dp"
        android:layout_toRightOf="@+id/heart_2"
        android:layout_below="@+id/face_3"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:paddingLeft="15.0dip"
        android:paddingRight="15.0dip"
        android:background="@drawable/face_4" />

    <TextView
        android:gravity="center"
        android:id="@+id/heart_5"
        android:layout_width="35dp"
        android:layout_height="35.0dip"
        android:background="@drawable/heart_5"
        android:layout_toRightOf="@+id/tv7"
        android:layout_below="@+id/tv6"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="12dp"
        android:paddingLeft="15.0dip"
        android:paddingRight="15.0dip"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/face_5"
        android:layout_width="35dp"
        android:layout_height="35.0dp"
        android:layout_toRightOf="@+id/heart_5"
        android:layout_below="@+id/face_4"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="25dp"
        android:paddingLeft="15.0dip"
        android:paddingRight="15.0dip"
        android:background="@drawable/face_5" />

    <Button
        android:id="@+id/b_1"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv2"
        android:background="#808080"
        android:layout_marginBottom="50.0dp"
        android:layout_marginRight="120dp"
        android:layout_marginTop="350dp"
        android:layout_marginLeft="125dp"
        android:textSize="16sp"
        android:text="提 交" />

</RelativeLayout>