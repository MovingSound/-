<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".frame_4"
    android:background="#F7F7F7"
    android:orientation="vertical">

    <TextView
        android:id="@+id/user_1"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:text="设置"
        android:textSize="19sp"
        android:textColor="#000000"
        android:gravity="left"
        android:paddingTop="16dp"
        android:paddingLeft="25dp"
        android:background="#F7F7F7"
        ></TextView>
    <Button
        android:id="@+id/user_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/user_1"
        android:background="#FFFFFF"
        android:text="账号与安全"
        android:gravity="left"
        android:textSize="15sp"
        android:paddingTop="14dp"
        android:layout_marginTop="10dp"
        android:paddingLeft="17dp"
        android:textColor="#000000"
        android:drawableRight="@drawable/go"
        android:paddingRight="20dp"
        android:paddingBottom="11dp"
        ></Button>
    <Button
        android:id="@+id/user_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/user_2"
        android:text="歌曲下载目录"
        android:gravity="left"
        android:textSize="15sp"
        android:paddingTop="14dp"
        android:layout_marginTop="10dp"
        android:background="#FFFFFF"
        android:textColor="#000000"
        android:paddingLeft="17dp"
        android:drawableRight="@drawable/go"
        android:paddingRight="20dp"
        android:paddingBottom="11dp"
        ></Button>
    <Button
        android:id="@+id/user_4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/user_3"
        android:text="音乐黑名单"
        android:gravity="left"
        android:textSize="15sp"
        android:paddingTop="14dp"
        android:layout_marginTop="10dp"
        android:background="#FFFFFF"
        android:textColor="#000000"
        android:paddingLeft="17dp"
        android:drawableRight="@drawable/go"
        android:paddingRight="20dp"
        android:paddingBottom="11dp"
        ></Button>
    <Button
        android:id="@+id/user_5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/user_4"
        android:text="个性化内容推送"
        android:gravity="left"
        android:textSize="15sp"
        android:paddingTop="14dp"
        android:layout_marginTop="10dp"
        android:background="#FFFFFF"
        android:paddingLeft="17dp"
        android:textColor="#000000"
        android:drawableRight="@drawable/go"
        android:paddingRight="20dp"
        android:paddingBottom="11dp"
        ></Button>
    <Button
        android:id="@+id/user_6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/user_5"
        android:text="默认音质"
        android:gravity="left"
        android:textSize="15sp"
        android:paddingTop="14dp"
        android:layout_marginTop="10dp"
        android:background="#FFFFFF"
        android:paddingLeft="17dp"
        android:textColor="#000000"
        android:drawableRight="@drawable/go"
        android:paddingRight="20dp"
        android:paddingBottom="11dp"
        ></Button>
    <Button
        android:id="@+id/user_7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/user_6"
        android:text="关于动声"
        android:gravity="left"
        android:textSize="15sp"
        android:paddingTop="14dp"
        android:layout_marginTop="10dp"
        android:background="#FFFFFF"
        android:paddingLeft="17dp"
        android:textColor="#000000"
        android:drawableRight="@drawable/go"
        android:paddingRight="20dp"
        android:paddingBottom="11dp"
        ></Button>
    <Button
        android:id="@+id/user_8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/user_7"
        android:text="帮助与反馈"
        android:gravity="left"
        android:textSize="15sp"
        android:paddingTop="14dp"
        android:layout_marginTop="10dp"
        android:background="#FFFFFF"
        android:paddingLeft="17dp"
        android:textColor="#000000"
        android:drawableRight="@drawable/go"
        android:paddingRight="20dp"
        android:paddingBottom="11dp"
        ></Button>
    <Button
        android:id="@+id/user_9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/user_8"
        android:background="#FFFFFF"
        android:text="切换账号"
        android:textColor="#000000"
        android:textSize="17dp"
        android:layout_marginTop="20dp"
        ></Button>
    <Button
        android:id="@+id/user_10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/user_9"
        android:text="退出"
        android:textSize="17dp"
        android:textColor="#000000"
        android:layout_marginTop="10dp"
        android:background="#FFFFFF"
        ></Button>

</RelativeLayout>